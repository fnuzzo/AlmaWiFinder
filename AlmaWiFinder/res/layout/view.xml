android:layout_x="6px"
android:layout_y="353px"
>
</TextView>
<view class="com.google.android.maps.MapView"
android:id="@+id/mapSpace"
android:layout_width="316px"
android:layout_height="209px"
android:layout_x="2px"
android:layout_y="3px"
android:enabled="true"
android:clickable="true"
android:apiKey="0iz2TbnxtVA8CAin_Z_RIJMw3p8vBHatGRvt-vA"
/>
</AbsoluteLayout>